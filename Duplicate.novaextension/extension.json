{
    "identifier": "online.wedro.Duplicate",
    "name": "Duplicate",
    "organization": "hutattedonmyarm",
    "description": "Duplicates the current selection, or the whole line of the cursor position",
    "version": "1.0.3",
    "categories": ["commands"],

    "main": "main.js",

    "activationEvents": ["onCommand:duplicate.duplicateSelection"],

    "entitlements": {
        "clipboard":false,
        "process": false,
        "requests": false
    },
    "min_runtime": "1.2",
    "commands": {
        "editor": [
            {
                "title": "Duplicate Selection",
                "command": "duplicate.duplicateSelection",
                "shortcut": "shift-cmd-d",
                "when": "editorHasFocus"
            }
        ]
    },

    "config": [],
    "homepage": "https://github.com/hutattedonmyarm/Duplicate",
    "bugs": "https://github.com/hutattedonmyarm/Duplicate/issues",
    "repository": "https://github.com/hutattedonmyarm/Duplicate",
    "license": "Mozilla Public License Version 2.0"
}
